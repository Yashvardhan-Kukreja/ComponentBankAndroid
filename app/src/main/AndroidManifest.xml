<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ieeevit.componentbankredefined">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/compbankicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/compbankicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.ieeevit.componentbankredefined.Activities.LogInActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme2"></activity>
        <activity
            android:name="com.ieeevit.componentbankredefined.Activities.Register"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme2" />
        <activity
            android:name="com.ieeevit.componentbankredefined.Activities.TabbedActivity"
            android:label="@string/title_activity_tabbed"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.ieeevit.componentbankredefined.Activities.EachComponentActivity"
            android:label="Component Details"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme3" />
        <activity
            android:name="com.ieeevit.componentbankredefined.Activities.SplashScreen"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ieeevit.componentbankredefined.Activities.AdminTabbedActivity"
            android:label="Admin"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>